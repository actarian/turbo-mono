{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": [
        "^dev"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "start": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    }
  },
  "globalDependencies": [
    "$NODE_ENV",
    "$SECRET_COOKIE_PASSWORD",
    "$STORE_STRATEGY",
    "$STORE_URL",
    "$STORE_API",
    "$STORE_BEARER",
    "$HOOKS_SECRET",
    "$NEXT_PUBLIC_URL",
    "$NEXT_PUBLIC_MOCK_FALLBACK_URL",
    "$NEXT_PUBLIC_API",
    "$NEXT_PUBLIC_GOOGLE_MAP_API_KEY"
  ]
}
