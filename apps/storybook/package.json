{
  "name": "storybook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:clean": "rimraf public/* \"!public/.gitkeep\"",
    "dev:copy": "copyfiles ../../public/**/* web/public",
    "dev:storybook": "start-storybook --ci -c .storybook -p 3002",
    "dev": "run-s dev:*",
    "start_": "serve storybook-dist",
    "build:clean": "rimraf public/* \"!public/.gitkeep\"",
    "build:copy": "copyfiles ../../public/**/* storybook/public",
    "build:storybook": "build-storybook --quiet -c .storybook -o .storybook-dist",
    "build_": "run-s build:*"
  },
  "dependencies": {
    "@websolute/store": "*",
    "@websolute/core": "*",
    "@websolute/forms": "*",
    "@websolute/hooks": "*",
    "@websolute/icons": "*",
    "@websolute/mock": "*",
    "@websolute/ui": "*",
    "next": "^12.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@next/bundle-analyzer": "^12.3.1",
    "@mdx-js/react": "^2.1.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/react": "^6.5.10",
    "@svgr/webpack": "^6.3.1",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@websolute/tsconfig": "*",
    "copyfiles": "^2.4.1",
    "eslint": "^8.23.0",
    "eslint-config-websolute": "*",
    "next-transpile-modules": "9.0.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "serve": "^14.0.1",
    "storybook-addon-next": "^1.6.7",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.8.2"
  }
}
