{
  "extends": "@websolute/tsconfig/tsreact.json",
  "include": ["."],
  "exclude": ["node_modules", "dist", "build"],
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@ui-components*": ["src/components*"],
      "@ui-components": ["src/components/index"],
      "@ui-core*": ["src/core*"],
      "@ui-core": ["src/core/index"],
      "@ui-forms*": ["src/forms*"],
      "@ui-forms": ["src/forms/index"],
      "@ui-hooks*": ["src/hooks*"],
      "@ui-hooks": ["src/hooks/index"],
      "@ui-icons*": ["src/icons*"],
      "@ui-icons": ["src/icons/index"],
      "@ui-sections*": ["src/sections*"],
      "@ui-sections": ["src/sections/index"],
      "@ui-styles*": ["src/styles*"],
      "@ui-styles": ["src/styles/index"]
    },
    "resolveJsonModule": true,
  }
}
